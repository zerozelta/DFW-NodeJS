{
  "compilerOptions": {
    "target": "es2020",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "types": [
      "express"
    ],
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "paths": {
      "#lib/*": [
        "./src/lib/*"
      ],
      "#repositories/*": [
        "./src/repositories/*"
      ],
      "#types/*": [
        "./src/types/*"
      ],
      "#listeners/*": [
        "./src/listeners/*"
      ],
      "#prisma/*": [
        "./src/prisma/*"
      ],
      "#makers/*": [
        "./src/makers/*"
      ],
      "#guards/*": [
        "./src/guards/*"
      ],
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".vscode"
  ]
}