{
  "name": "@scefira/dfw",
  "version": "1.4.1",
  "description": "NodeJS fast API prototiping framework",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist/**/*",
    "prisma/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "imports": {
    "#lib/*": {
      "import": "./dist/esm/lib/*.js",
      "require": "./dist/cjs/lib/*.js"
    },
    "#repositories/*": {
      "import": "./dist/esm/repositories/*.js",
      "require": "./dist/cjs/repositories/*.js"
    },
    "#listeners/*": {
      "import": "./dist/esm/listeners/*.js",
      "require": "./dist/cjs/listeners/*.js"
    },
    "#makers/*": {
      "import": "./dist/esm/makers/*.js",
      "require": "./dist/cjs/makers/*.js"
    },
    "#types/*": {
      "import": "./dist/esm/types/*.js",
      "require": "./dist/cjs/types/*.js"
    },
    "#guards/*": {
      "import": "./dist/esm/guards/*.js",
      "require": "./dist/cjs/guards/*.js"
    }
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/test.ts",
    "dev:prisma": "dotenv -e .env.development -- npx prisma studio",
    "dev:sync": "npx prisma db push",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rimraf dist && npm run build:cjs && npm run build:esm",
    "postbuild": "node scripts/mark-esm.mjs",
    "prepublishOnly": "npm run build",
    "check:esm": "node scripts/check-exports.mjs"
  },
  "author": "Scefira Technologies",
  "license": "ISC",
  "peerDependencies": {
    "@prisma/client": "^7",
    "zod": "^4"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/adapter-pg": "^7",
    "@types/busboy": "^1.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-session": "^1.18.2",
    "@types/luxon": "^3",
    "@types/passport": "^1.0.17",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^5",
    "express-fileupload": "^1.5.2",
    "express-session": "^1.18.2",
    "lru.min": "^1.1.2",
    "luxon": "^3",
    "md5-file": "^5.0.0",
    "mime-types": "^2.1.35",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.4.2",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^8.3.4",
    "madge": "^8.0.0",
    "prisma": "^7",
    "rimraf": "^6",
    "tsx": "^4",
    "typescript": "^5"
  }
}
